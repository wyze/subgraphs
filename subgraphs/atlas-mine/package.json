{
  "name": "atlas-mine",
  "version": "1.0.0",
  "description": "A subgraph for Atlas Mine.",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "create": "graph create --node $GRAPH_NODE_ENDPOINT atlas-mine",
    "deploy": "graph deploy atlas-mine --ipfs $GRAPH_IPFS_ENDPOINT --node $GRAPH_NODE_ENDPOINT --version-label v0.0.0",
    "prepare": "run-s -l prepare:*",
    "prepare:constants": "yarn --cwd ../../packages/constants prepare:arbitrum",
    "prepare:subgraph": "mustache ../../node_modules/@shared/subgraph-config/src/arbitrum.json template.yaml > subgraph.yaml",
    "test": "exit 0 && graph test"
  }
}
